{
  "firestore": {
    "rules": "firebase_config/firestore.rules",
    "indexes": "firebase_config/firestore.indexes.json"
  },
  "functions": [
    {
      "runtime": "nodejs16",
      "source": ".output/server"
    }
  ],
  "emulators": {
    "auth": {
      "port": 9095
    },
    "functions": {
      "port": 5005
    },
    "firestore": {
      "port": 8085
    },
    "ui": {
      "enabled": true
    }
  },
  "hosting": {
    "site": "prov-xyz-dev",
    "public": ".output/public",
    "cleanUrls": true,
    "rewrites": [{
      "source": "**",
      "function": "server"
    }],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}
